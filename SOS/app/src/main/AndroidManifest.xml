<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.uj.sos">
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.SEND_SMS"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/sos"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/sos"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".sirenActivity" />
        <activity android:name=".whistleActivity" />
        <activity android:name=".addcontactActivity" />
        <activity
            android:name=".panicActivity"
            android:label="@string/title_activity_panic"
            android:theme="@style/AppTheme"></activity>
    </application>

</manifest>